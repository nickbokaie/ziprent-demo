<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Social Sharing Meta Tags -->
    <meta property="og:title" content="Ziprent">
    <meta property="og:description" content="Automated tenant placement service">
    <meta property="og:image" content="images/ziprent-thumbnail-new.png">
    <meta property="og:url" content="ziprent.com">
    <meta name="twitter:card" content="images/ziprent-thumbnail-new.png">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "Ziprent" %></title>
    <%= stylesheet_link_tag "normalize" %>
    <%= stylesheet_link_tag "lp" %>
    <script src="/javascripts/scrollreveal.min.js"></script>

    <!-- Favicon -->
    <link rel="icon" href="/images/favicon.png" type="image/x-icon" />
    <link rel="shortcut icon" href="/images/favicon.png" type="image/x-icon" />

    <!-- Slick Slider Stuff -->
    <!-- Slick Slider Stuff-->
    <link rel="stylesheet" type="text/css" href="/stylesheets/slick.css"/>
    <link rel="stylesheet" type="text/css" href="/stylesheets/slick-theme.css"/>

  
    <script>window.sr = ScrollReveal();</script>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
  </head>
  <body>
    <%= yield %>
    <%= partial 'partials/lp/three-col' %>
    <%= partial 'partials/lp/tenant-score' %>
    <%= partial 'partials/lp/testimonials' %>
    <%= partial 'partials/lp/bottom-cta' %>
    <%= partial 'partials/lp/footer' %>

    <script>
    sr.reveal('.img-reveal', { duration: 1500 });
  </script>

  <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
  <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
  <script type="text/javascript" src="/javascripts/slick.min.js"></script>
  <%= javascript_include_tag "site" %>
  </body>
</html>